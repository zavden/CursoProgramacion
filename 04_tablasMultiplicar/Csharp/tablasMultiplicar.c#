using System;
public class tablasMultiplicar
{
    public static void Main(String[] args)
    {
        int i,j;
        Console.Write("Ingresa el numero de la tabla que deseas conocer: ");
        j=Convert.ToInt32(Console.ReadLine());
        for(i=1;i<=10;i++)
        {
            Console.WriteLine("{0} x {1} = {2}",j,i,i*j);
        }
    }
}