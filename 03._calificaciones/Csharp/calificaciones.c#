using System;
public class calificaciones
{
    public static void Main(String[] args)
    {
        double calif;
        Console.Write("Ingresa tu calificacion: ");
        calif=Convert.ToDouble(Console.ReadLine());
        if(calif<6)
        {
            if(calif<0)
                Console.WriteLine("Tu calificacion no es valida.");
            else
                Console.WriteLine("Estas reprobado.");
        }
        else
        {
            if(calif>10)
                Console.WriteLine("Tu calificacion no es valida.");
            else
                Console.WriteLine("Estas aprobado.");
        }
    }
}